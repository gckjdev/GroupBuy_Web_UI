<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.pageListWidget {
			padding: 40px 0;
			text-align: center;
		}
		
		.anchor {
			margin: 0 8px;
		}
	</ui:style>

	<g:HorizontalPanel>
		<g:Anchor styleName='{style.anchor}' ui:field='previousPage'
			href='javascript:;'>
			&lt; </g:Anchor>
		<g:Label ui:field='description'></g:Label>
		<g:Anchor styleName='{style.anchor}' ui:field='nextPage'
			href='javascript:;'>
			&gt; </g:Anchor>
	</g:HorizontalPanel>
</ui:UiBinder>

